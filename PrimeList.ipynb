      "source": [
        "n = int(input(\"Lütfen kaça kadar olan asal sayıları listelemek istediğinizi belirten bir tam sayı giriniz:\"))\n",
        "asal=[]\n",
        "for i in range(2,n+1):\n",
        "    prime = True\n",
        "    for j in range(2,i):\n",
        "        if (i%j==0):\n",
        "            prime = False\n",
        "    if prime:\n",
        "       asal.append(i)\n",
        "print (asal)"
      ],
