      "source": [
        "sayi = int(input(\"Lütfen bir tam sayı giriniz: \"))\n",
        "if sayi > 1:\n",
        "  for i in range(2, sayi//2):\n",
        "    if sayi % i ==0:\n",
        "      print (sayi, \"is not a prime number.\")\n",
        "      break\n",
        "  else:\n",
        "    print(sayi, \"is a prime number.\")\n",
        "else:\n",
        "  print (sayi, \"is not a prime number.\")"
      ],
